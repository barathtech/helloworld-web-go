pipeline {
    agent any
    tools {
        go '1.18.1'
    }
     stages { 
      stage('Build') {
        steps {
               git 'https://github.com/barathtech/helloworld-web-go.git'
            }
        }
      stage('Test') {
        steps {
             sh 'go version '
             sh 'nohup go run server.go & '
             }
          }
       } 
    } 
